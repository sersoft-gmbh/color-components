      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "On iOS, the actions are displayed as items after the content",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "of the section. On macOS, the actions are displayed",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "when a user hovers over the section.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "The following example adds an ‘Add’ button to",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "the ‘Categories’ section.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "List {",
            "    Label(\"Home\", systemImage: \"house\")",
            "    Label(\"Alerts\", systemImage: \"bell\")",
            "",
            "    Section(\"Categories\") {",
            "        Label(\"Climate\", systemImage: \"fan\")",
            "        Label(\"Lights\", systemImage: \"lightbulb\")",
            "    }",
            "    .sectionActions {",
            "        Button(\"Add Category\", systemImage: \"plus\") { }",
            "    }",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
