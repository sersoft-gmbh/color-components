      "parameters" : [
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "A binding to whether the interface should be shown.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "isPresented"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The list of supported content types which can",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "be imported.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "allowedContentTypes"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Whether the importer allows the user to",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "select more than one file to import.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "allowsMultipleSelection"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "A callback that will be invoked when the operation has",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "succeeded or failed. To access the received URLs, call ",
                  "type" : "text"
                },
                {
                  "code" : "startAccessingSecurityScopedResource",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "When the access is no longer required, call ",
                  "type" : "text"
                },
                {
                  "code" : "stopAccessingSecurityScopedResource",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "onCompletion"
        }
      ]
    },
    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "In order for the interface to appear, ",
              "type" : "text"
            },
            {
              "code" : "isPresented",
              "type" : "codeVoice"
            },
            {
              "text" : " must be ",
              "type" : "text"
            },
            {
              "code" : "true",
              "type" : "codeVoice"
            },
            {
              "text" : ". When",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "the operation is finished, ",
              "type" : "text"
            },
            {
              "code" : "isPresented",
              "type" : "codeVoice"
            },
            {
              "text" : " will be set to ",
              "type" : "text"
            },
            {
              "code" : "false",
              "type" : "codeVoice"
            },
            {
              "text" : " before",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "code" : "onCompletion",
              "type" : "codeVoice"
            },
            {
              "text" : " is called. If the user cancels the operation,",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "code" : "isPresented",
              "type" : "codeVoice"
            },
            {
              "text" : " will be set to ",
              "type" : "text"
            },
            {
              "code" : "false",
              "type" : "codeVoice"
            },
            {
              "text" : " and ",
              "type" : "text"
            },
            {
              "code" : "onCompletion",
              "type" : "codeVoice"
            },
            {
              "text" : " will not be",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "called.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "This dialog provides security-scoped URLs.",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "Call the ",
                  "type" : "text"
                },
                {
                  "code" : "startAccessingSecurityScopedResource",
                  "type" : "codeVoice"
                },
                {
                  "text" : " method to access or bookmark",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "the URLs, and the ",
                  "type" : "text"
                },
                {
                  "code" : "stopAccessingSecurityScopedResource",
                  "type" : "codeVoice"
                },
                {
                  "text" : " method",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "to release the access.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Note",
          "style" : "note",
          "type" : "aside"
        },
        {
          "inlineContent" : [
            {
              "text" : "For example, a button that allows the user to choose multiple PDF files for the application",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "to combine them later, might look like this:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "   struct PickPDFsButton: View {",
            "       @State private var showFileImporter = false",
            "       var handlePickedPDF: (URL) -> Void",
            "",
            "       var body: some View {",
            "           Button {",
            "               showFileImporter = true",
            "           } label: {",
            "               Label(\"Choose PDFs to combine\", systemImage: \"doc.circle\")",
            "           }",
            "           .fileImporter(",
            "               isPresented: $showFileImporter,",
            "               allowedContentTypes: [.pdf],",
            "               allowsMultipleSelection: true",
            "           ) { result in",
            "               switch result {",
            "               case .success(let files):",
            "                   files.forEach { file in",
            "                       \/\/ gain access to the directory",
            "                       let gotAccess = file.startAccessingSecurityScopedResource()",
            "                       if !gotAccess { return }",
            "                       \/\/ access the directory URL",
            "                       \/\/ (read templates in the directory, make a bookmark, etc.)",
            "                       handlePickedPDF(file)",
            "                       \/\/ release access",
            "                       file.stopAccessingSecurityScopedResource()",
            "                   }",
            "               case .failure(let error):",
            "                   \/\/ handle error",
            "                   print(error)",
            "               }",
            "           }",
            "       }",
            "   }"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Changing ",
                  "type" : "text"
                },
                {
                  "code" : "allowedContentTypes",
                  "type" : "codeVoice"
                },
                {
                  "text" : " or ",
                  "type" : "text"
                },
                {
                  "code" : "allowsMultipleSelection",
                  "type" : "codeVoice"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "while the file importer is presented will have no immediate effect,",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "however will apply the next time it is presented.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
