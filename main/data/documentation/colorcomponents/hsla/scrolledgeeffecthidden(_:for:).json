          "inlineContent" : [
            {
              "text" : "By default, a scroll view renders an automatic edge effect style.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "Use this modifier to hide any edge effects for scroll views",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "within this hierarchy.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "ScrollView {",
            "    LazyVStack {",
            "        ForEach(data) { item in",
            "            RowView(item)",
            "        }",
            "    }",
            "}",
            ".scrollEdgeEffectHidden()"
          ],
