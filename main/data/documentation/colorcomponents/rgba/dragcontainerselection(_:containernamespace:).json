    },
    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "A drag container finds the nearest enclosing ",
              "type" : "text"
            },
            {
              "code" : "dragContainerSelection(_:containerNamespace:)",
              "type" : "codeVoice"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "with the same item identifier type and same namespace, if specified.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "Drag container uses the provided selected item identifiers to determine what",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "the drag payload should be.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "If the dragged view is associated with a selected identifier, the payload should contain",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "all the selected items. If the dragged view is not selected, the payload should",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "not contain the whole selection, just the dragged item.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "With ",
              "type" : "text"
            },
            {
              "code" : "dragContainerSelection(_:containerNamespace:)",
              "type" : "codeVoice"
            },
            {
              "text" : ", you get fine-grained control",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "over what items are included in the drag payload.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "    struct FruitContainer: View {",
            "         @State private var fruits: [Fruit]",
            "         @State private var selection: [Fruit.ID]",
            "",
            "         var body: some View {",
            "             VStack {",
            "                 ForEach(fruits) { fruit in",
            "                     FruitView(fruit)",
            "                         .draggable(containerItemID: fruit.id)",
            "                  }",
            "              }",
            "             .dragContainer(for: Fruit.self) { ids in",
            "                 fruits(with: ids)",
            "             }",
            "             .dragContainerSelection(selection)",
            "         }",
            "",
            "       func fruits(with ids: [Fruit.ID]) -> [Fruit] { ... }",
            "",
            "       struct Fruit: Transferable, Identifiable {",
            "           let id: String",
            "           ...",
            "       }",
            "",
            "       struct FruitView: View {",
            "           init(_ fruit: Fruit) { ... }",
            "       }",
            "   }"
          ],
          "syntax" : null,
          "type" : "codeListing"
        }
      ],
