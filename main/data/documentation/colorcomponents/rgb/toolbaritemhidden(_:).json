                {
                  "text" : "Whether the view in a control group toolbar item is",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "hidden.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
