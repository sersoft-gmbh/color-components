      "parameters" : [
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The system-provided Rotor that will be overridden",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "by this custom Rotor.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "systemRotor"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "An array of ranges that will be used to generate",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "the entries of the Rotor.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "textRanges"
        }
      ]
    },
    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "An Accessibility Rotor is a shortcut for Accessibility users to",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "quickly navigate to specific elements of the user interface,",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "and optionally specific ranges of text within those elements.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "In the following example, a Message application adds a Rotor allowing",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "the user to navigate through all the ranges of text containing headings.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "extension Message {",
            "    \/\/ Ranges of special areas in the `content` text. Calculated when",
            "    \/\/ `content` is set and then cached so that we don't have to",
            "    \/\/ re-compute them.",
            "    var headingRanges: [Range<String.Index>]",
            "}",
            "",
            "struct MessageContentView: View {",
            "    TextEditor(.constant(message.content))",
            "        .accessibilityRotor(",
            "            .heading,",
            "            textRanges: message.headingRanges",
            "        )",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
    }
