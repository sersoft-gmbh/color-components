            {
              "inlineContent" : [
                {
                  "text" : "Localized text describing to the user what",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "is contained in this additional information entry. For example:",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "“orientation”.",
                  "type" : "text"
