          "code" : [
            "struct TapGestureExample: View {",
            "    @State private var location: CGPoint = .zero",
            "",
            "    var body: some View {",
            "        Circle()",
            "            .fill(self.location.y > 50 ? Color.blue : Color.red)",
            "            .frame(width: 100, height: 100, alignment: .center)",
            "            .onTapGesture { location in",
