      "parameters" : [
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "A binding to whether the interface should be shown.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "isPresented"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The list of supported content types which can",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "be imported.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "allowedContentTypes"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "A callback that will be invoked when the operation has",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "succeeded or failed. To access the received URLs, call ",
                  "type" : "text"
                },
                {
                  "code" : "startAccessingSecurityScopedResource",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "When the access is no longer required, call ",
                  "type" : "text"
                },
                {
                  "code" : "stopAccessingSecurityScopedResource",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "onCompletion"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "A ",
                  "type" : "text"
                },
                {
                  "code" : "Result",
                  "type" : "codeVoice"
                },
                {
                  "text" : " indicating whether the operation succeeded or",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "failed.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "result"
        }
      ]
    },
    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "In order for the interface to appear, ",
              "type" : "text"
            },
            {
              "code" : "isPresented",
              "type" : "codeVoice"
            },
            {
              "text" : " must be ",
              "type" : "text"
            },
            {
              "code" : "true",
              "type" : "codeVoice"
            },
            {
              "text" : ". When",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "the operation is finished, ",
              "type" : "text"
            },
            {
              "code" : "isPresented",
              "type" : "codeVoice"
            },
            {
              "text" : " will be set to ",
              "type" : "text"
            },
            {
              "code" : "false",
              "type" : "codeVoice"
            },
            {
              "text" : " before",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "code" : "onCompletion",
              "type" : "codeVoice"
            },
            {
              "text" : " is called. If the user cancels the operation,",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "code" : "isPresented",
              "type" : "codeVoice"
            },
            {
              "text" : " will be set to ",
              "type" : "text"
            },
            {
              "code" : "false",
              "type" : "codeVoice"
            },
            {
              "text" : " and ",
              "type" : "text"
            },
            {
              "code" : "onCompletion",
              "type" : "codeVoice"
            },
            {
              "text" : " will not be",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "called.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "This dialog provides security-scoped URLs.",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "Call the ",
                  "type" : "text"
                },
                {
                  "code" : "startAccessingSecurityScopedResource",
                  "type" : "codeVoice"
                },
                {
                  "text" : " method to access or bookmark",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "the URLs, and the ",
                  "type" : "text"
                },
                {
                  "code" : "stopAccessingSecurityScopedResource",
                  "type" : "codeVoice"
                },
                {
                  "text" : " method",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "to release the access.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Note",
          "style" : "note",
          "type" : "aside"
        },
        {
          "inlineContent" : [
            {
              "text" : "For example, an application can have a button that allows the user to choose the default directory",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "with document templates loaded on every launch. Such a button might look like this:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            " struct PickTemplatesDirectoryButton: View {",
            "     @State private var showFileImporter = false",
            "     var onTemplatesDirectoryPicked: (URL) -> Void",
            "",
            "     var body: some View {",
            "         Button {",
            "             showFileImporter = true",
            "         } label: {",
            "             Label(\"Choose templates directory\", systemImage: \"folder.circle\")",
            "         }",
            "         .fileImporter(",
            "             isPresented: $showFileImporter,",
            "             allowedContentTypes: [.directory]",
            "         ) { result in",
            "              switch result {",
            "              case .success(let directory):",
            "                  \/\/ gain access to the directory",
            "                  let gotAccess = directory.startAccessingSecurityScopedResource()",
            "                  if !gotAccess { return }",
            "                  \/\/ access the directory URL",
            "                  \/\/ (read templates in the directory, make a bookmark, etc.)",
            "                  onTemplatesDirectoryPicked(directory)",
            "                  \/\/ release access",
            "                  directory.stopAccessingSecurityScopedResource()",
            "              case .failure(let error):",
            "                  \/\/ handle error",
            "                  print(error)",
            "              }",
            "         }",
            "     }",
            " }"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Changing ",
                  "type" : "text"
                },
                {
                  "code" : "allowedContentTypes",
                  "type" : "codeVoice"
                },
                {
                  "text" : " while the file importer is",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "presented will have no immediate effect, however will apply the next",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "time it is presented.",
                  "type" : "text"
                }
