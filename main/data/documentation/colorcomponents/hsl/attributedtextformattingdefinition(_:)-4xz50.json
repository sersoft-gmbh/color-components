        {
          "inlineContent" : [
            {
              "text" : "Using only an attribute scope for the text formatting definition allows",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "the editor to use all attributes in the scope, but does not constrain",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "their values in any way. To constrain what values attribute can have,",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "use ",
              "type" : "text"
            },
