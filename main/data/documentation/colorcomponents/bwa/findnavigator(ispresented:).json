              "type" : "codeVoice"
            },
            {
              "text" : " binding to ",
              "type" : "text"
            },
            {
              "code" : "true",
              "type" : "codeVoice"
            },
            {
              "text" : " has",
              "type" : "text"
            },
            {
              "text" : " ",
