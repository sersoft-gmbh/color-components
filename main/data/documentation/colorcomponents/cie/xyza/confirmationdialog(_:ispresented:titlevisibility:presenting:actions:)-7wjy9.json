            "}",
            "struct ConfirmFileImport: View {",
            "    var title: String",
            "    @State private var isConfirming = false",
            "    @State private var dialogDetail: FileDetails?",
            "    var body: some View {",
            "        Button(\"Import File\") {",
            "            dialogDetail = FileDetails(",
            "                name: \"MyImageFile.png\", fileType: .png)",
            "            isConfirming = true",
            "        }",
