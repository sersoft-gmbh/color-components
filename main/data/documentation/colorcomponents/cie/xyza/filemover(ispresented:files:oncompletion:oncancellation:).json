        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "A callback that will be invoked when the operation has",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "succeeded or failed. The ",
                  "type" : "text"
                },
                {
                  "code" : "result",
                  "type" : "codeVoice"
                },
                {
                  "text" : " indicates whether",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "the operation succeeded or failed.",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "To access the received URLs, call ",
                  "type" : "text"
                },
                {
                  "code" : "startAccessingSecurityScopedResource",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "When the access is no longer required, call ",
                  "type" : "text"
                },
                {
                  "code" : "stopAccessingSecurityScopedResource",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
