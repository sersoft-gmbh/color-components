              "inlineContent" : [
                {
                  "text" : "Whether the suppression toggle is on or off",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "in the dialog.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
