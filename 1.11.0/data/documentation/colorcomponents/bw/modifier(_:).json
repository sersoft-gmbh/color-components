            },
            {
              "text" : ", to",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "create a new view. For example, if you create a view modifier for",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "a new kind of caption with blue text surrounded by a rounded rectangle:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "struct BorderedCaption: ViewModifier {",
            "    func body(content: Content) -> some View {",
            "        content",
            "            .font(.caption2)",
            "            .padding(10)",
            "            .overlay(",
            "                RoundedRectangle(cornerRadius: 15)",
            "                    .stroke(lineWidth: 1)",
            "            )",
            "            .foregroundColor(Color.blue)",
            "    }",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "You can use ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/ColorComponents\/documentation\/ColorComponents\/BW\/modifier(_:)",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " to extend ",
              "type" : "text"
            },
            {
              "code" : "View",
              "type" : "codeVoice"
            },
            {
              "text" : " to create new modifier",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "for applying the ",
              "type" : "text"
            },
            {
              "code" : "BorderedCaption",
              "type" : "codeVoice"
            },
            {
              "text" : " defined above:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "extension View {",
            "    func borderedCaption() -> some View {",
            "        modifier(BorderedCaption())",
            "    }",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Then you can apply the bordered caption to any view:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
